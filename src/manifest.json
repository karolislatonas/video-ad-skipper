{
    "name": "Ad-Skipper",
    "short_name": "Short Name",
	"version": "0.1",
    "manifest_version": 2,
    "browser_action": {
        
    },
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
	"content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}